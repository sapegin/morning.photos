{
  "name": "morning-photos",
  "description": "Artem Sapegin Photography",
  "author": "Artem Sapegin <artem@sapegin.ru> (https://sapegin.me/)",
  "license": "SEE LICENSE IN <Readme.md>",
  "private": true,
  "scripts": {
    "start": "npx --node-arg '-r esm -r dotenv/config' gatsby develop",
    "build": "npx --node-arg '-r esm' gatsby build",
    "serve": "npx --node-arg '-r esm' gatsby serve",
    "pretest": "npm run lint:js && npm run lint:css && npm run lint:md",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "typecheck": "tsc",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "lint:md": "remark --quiet \"content/**/*.md\"",
    "lint:js": "eslint . --cache --fix --ext .js,.tsx",
    "format": "prettier --write \"**/*.{js,tsx,md}\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "engines": {
    "node": ">=12"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "array-shuffle": "^1.0.1",
    "death": "^1.1.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-group": "^3.0.2",
    "react-helmet": "^5.2.1",
    "react-photo-gallery": "^8.0.0",
    "styled-components": "^5.0.0",
    "tamia": "^5.11.4",
    "tamia-gatsby-link": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^13.1.7",
    "@types/react": "^16.9.17",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "cloudinary": "^1.23.0",
    "devenv": "^1.0.1",
    "download": "^7.1.0",
    "eslint": "~6.8.0",
    "eslint-config-tamia": "~7.1.1",
    "esm": "^3.2.4",
    "exifr": "^5.0.6",
    "fs-extra": "^8.1.0",
    "gatsby": "2.19.2",
    "gatsby-plugin-goatcounter": "^0.3.1",
    "gatsby-plugin-netlify": "^2.1.31",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-styled-components": "^3.1.18",
    "gatsby-plugin-typescript": "^2.1.26",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "get-image-colors": "^2.0.0",
    "glob": "~7.1.6",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "lodash": "~4.17.21",
    "prettier": "^1.19.1",
    "probe-image-size": "^5.0.0",
    "react-styleguidist": "^13.0.0",
    "remark-cli": "~7.0.1",
    "remark-lint": "~6.0.5",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "unist-util-visit": "~2.0.1"
  },
  "lint-staged": {
    "*.{js,tsx,md,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,tsx}": [
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ]
  }
}
