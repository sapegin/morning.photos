{
  "name": "morning-photos",
  "description": "Artem Sapegin Photography",
  "author": "Artem Sapegin <artem@sapegin.ru> (https://sapegin.me/)",
  "license": "SEE LICENSE IN <Readme.md>",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "pretest": "npm run lint:js && npm run lint:md",
    "test": "npm run typecheck",
    "posttest": "npm run format",
    "typecheck": "tsc",
    "lint:md": "remark --quiet \"content/**/*.md\"",
    "lint:js": "eslint . --cache --fix --ext .js,.tsx",
    "format": "prettier --loglevel warn --write \"**/*.{js,ts,tsx,md}\"",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "engines": {
    "node": ">=16"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@types/fs-extra": "^9.0.13",
    "array-shuffle": "^1.0.1",
    "death": "~1.1.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-group": "^3.0.2",
    "react-photo-gallery": "^8.0.0",
    "styled-components": "~5.3.6",
    "tamia": "^5.13.1",
    "tamia-gatsby-link": "^1.0.6"
  },
  "devDependencies": {
    "@types/death": "^1.1.2",
    "@types/node": "^16.0.0",
    "@types/probe-image-size": "^7.2.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "~5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cloudinary": "^1.23.0",
    "devenv": "^1.0.1",
    "download": "^7.1.0",
    "eslint": "^8.23.0",
    "eslint-config-tamia": "^8.0.1",
    "eslint-plugin-react": "^7.31.6",
    "exifr": "~7.1.3",
    "fs-extra": "^8.1.0",
    "gatsby": "^4.20.0",
    "gatsby-plugin-goatcounter": "^0.4.0",
    "gatsby-plugin-image": "2.20.0",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-styled-components": "^5.20.0",
    "gatsby-source-filesystem": "^4.0.0",
    "gatsby-transformer-remark": "^4.0.0",
    "get-image-colors": "~4.0.1",
    "glob": "~7.1.6",
    "husky": "^4.0.10",
    "lint-staged": "^9.5.0",
    "lodash": "~4.17.21",
    "prettier": "^2.7.1",
    "probe-image-size": "~7.2.3",
    "react-styleguidist": "^13.0.0",
    "remark-cli": "~7.0.1",
    "remark-lint": "~6.0.5",
    "typescript": "4.7.4",
    "unist-util-visit": "~2.0.1"
  },
  "lint-staged": {
    "*.{js,tsx,md,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,tsx}": [
      "eslint --fix",
      "stylelint --fix",
      "git add"
    ]
  }
}
